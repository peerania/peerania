#!/usr/bin/env bash
# Execute a commadn against a bitcoin node

# compile
eosiocpp -o /media/nikita/main/peerania/src/contracts/peerania/peerania.wast /media/nikita/main/peerania/src/contracts/peerania/peerania.cpp

# generate abi
eosiocpp -g /media/nikita/main/peerania/src/contracts/peerania/peerania.abi /media/nikita/main/peerania/src/contracts/peerania/peerania.hpp