#!/usr/bin/env bash
set -ex

cleos push action peerania.dev registeracc '{"user": "bob", "display_name": "Bobby", "ipfs_profile": "QmYBPXQMG32aUVqtMuptyR4dzgjykmBozyuDbvRGXxc1DK"}' -p bob
