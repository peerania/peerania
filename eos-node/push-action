do
    cleos push action peeranhamain chnguserrt '{"user": "alice", "rating_change": '$(( 5*$i ))'}' -p bob
    cleos push action peeranhamain chnguserrt '{"user": "user1", "rating_change": '$(( 10*$i ))'}' -p user1
