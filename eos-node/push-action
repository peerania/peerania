    then
        cleos push action peeranhatken pickupreward '{"user": "alice", "period": '$(( $i - 2 ))'}' -p alice
        cleos push action peeranhatken pickupreward '{"user": "user1", "period": '$(( $i - 2 ))'}' -p user1
    fi
    sleep 3
done
